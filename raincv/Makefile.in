prefix= @prefix@
srcdir= @srcdir@
bindir= @bindir@
sbindir= @sbindir@

CFLAGS= @CFLAGS@ -DPROJECT=\"${TARGET}\"
CFLAGS+= -I../libdzf/
CFLAGS+= -Werror

LDFLAGS= @LDFLAGS@
LIBOBJS= @LIBOBJS@
LIBS=    @LIBS@
CC=      @CC@

TARGET= raincv

ifneq ("$(wildcard ./Makefile.custom)", "")
	include Makefile.custom
endif

INSTALL= @INSTALL@

BINOBJS= main.o rcv-utils.o

all:	$(TARGET)
raincv: $(BINOBJS)
	$(CC) $(LDFLAGS) -o $@ $(BINOBJS) $(LIBS)

